<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :expected_due_date %>
    <%= hidden_field_tag "project[manager_id]", current_member.id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "button alert" %>
  </div>
<% end %>
